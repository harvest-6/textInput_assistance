{
    "manifest_version": 1,
    "version": "1.0",
    "type": "APP",
    "name": {
        "en": "textInput_assistance",
        "ja": "入力補助プラグイン"
    },
    "description": {
        "en": "It is a plug-in that adds input auxiliary functions that can be selected multiple times.",
        "ja": "複数選択可能な入力補助機能を追加するプラグインです。"
    },
    "icon": "image/icon.png",
    "homepage_url": {
        "ja": "https://qiita.com/necomochi/items/0ba94f6e9a9762aed5f1"
    },
    "desktop": {
        "js": [
            "https://unpkg.com/kintone-ui-component/umd/kuc.min.js",
            "js/desktop.js"
        ],
        "css": [
            "css/desktop.css"
        ]
    },
    "config": {
        "html": "html/config.html",
        "js": [
            "https://js.cybozu.com/jquery/1.11.1/jquery.min.js",
            "js/kintone-config-helper.js",
            "https://js.cybozu.com/jsrender/1.0.6/jsrender.min.js",
            "js/config.js"
        ],
        "css": [
                "css/config.css",
                "css/51-modern-default.css"
        ],
        "required_params": ["displayspace","enterfield"]
    }
}
